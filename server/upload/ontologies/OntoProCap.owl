    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#PhaseChangeOfLiquids">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#PhaseChange"/>
    </owl:Class>
    


