        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Combination"/>
        <rdfs:comment xml:lang="en">The process of soaking a porous material with a liquid.</rdfs:comment>
    </owl:Class>
    


