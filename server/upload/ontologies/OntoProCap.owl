        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Particles are separated from gas by impingement on collected bodies arrayed across the path of the gas stream.(Perry)</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Impregnation -->

