    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Abrasion -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Abrasion">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Fragmentation"/>
        <rdfs:comment xml:lang="en">Granulation of a solid by frictional action using an abrading medium.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Absorption -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Absorption">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Absorption is the transfer of a soluble component in a gas-phase mixture into a liquid absorbent whose volatility is low under process conditions. (Perry)</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Adsorption -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Adsorption">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Adsorption is the selcective transfer of molecules or fine-granular particles from a fluid (gas or liquid) to the interface of the adsorbents.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Agitating -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Agitating">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#MixingOfLiquids"/>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#BoilingDown -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#BoilingDown">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Boiling down refers to the separation of a solution into gaseous and solid substances by evaporating the volatile components completely.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Briquetting -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Briquetting">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Combination"/>
        <rdfs:comment xml:lang="en">The agglomeration of a fine-granular powder to coarse-granular pieces by mechanical pressure.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Capability -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Capability"/>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Cementation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Cementation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Cementation is process to recover dissolved metals from the aqueous phase. The basic principle of cementation is contact reduction. In other words, the electrons from a less noble metal are given up to a more noble dissolved metal. (Free, M. L. (2013). Hydrometallurgy: Fundamentals and Applications. Wiley.)</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Centrifugation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Centrifugation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">A process of separating the lighter portions of a solution, mixture, or suspension from the heavier portions by centrifugal force.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Circulating -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Circulating">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#MixingOfLiquids"/>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Combination -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Combination">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TransformationCapability"/>
        <rdfs:comment xml:lang="en">A combination in terms of unit operation means to get together different process steps to achieve a particular one.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Compacting -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Compacting">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Combination"/>
        <rdfs:comment xml:lang="en">The compression of a loose, voluminous powder to a more dense, closely packed powder by mechanical pressure.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#ConcentrationByEvaporation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#ConcentrationByEvaporation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">In evaporization, the vapor from a boiling liquid is removed and a more concentrated solution remains. (Geankoplis: Transport processes and unit operations)</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Condensation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Condensation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#PhaseChangeOfGases"/>
        <owl:disjointWith rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Desublimation"/>
        <rdfs:comment xml:lang="en">The phase transition from gas to liquid.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Conveying -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Conveying">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TransportCapability"/>
        <owl:disjointWith rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Dosing"/>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#CoolingOfGases -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#CoolingOfGases">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TemperatureChangeOfGases"/>
        <owl:disjointWith rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#HeatingOfGases"/>
        <rdfs:comment xml:lang="en">Reducing the temperature of a gas without changing ist state of aggregation</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#CoolingOfLiquids -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#CoolingOfLiquids">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TemperatureChangeOfLiquids"/>
        <owl:disjointWith rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#HeatingOfLiquids"/>
        <rdfs:comment xml:lang="en">Reducing the temperature of a liquid without changing ist state of aggregation</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#CoolingOfSolids -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#CoolingOfSolids">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TemperatureChangeOfSolids"/>
        <owl:disjointWith rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#HeatingOfSolids"/>
        <rdfs:comment xml:lang="en">Reducing the temperature of a solid.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Cutting -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Cutting">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Fragmentation"/>
        <rdfs:comment xml:lang="en">Fragmentation of solid material by the effect of shear forces</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#CyclonicSeparation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#CyclonicSeparation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Cyclonic separation is a method of removing particles from a gas stream by means of centrifugal forces.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Desublimation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Desublimation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#PhaseChangeOfGases"/>
        <rdfs:comment xml:lang="en">The phase transition from gas to solid without passing through an intermediate liquid state.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#DiffusionSeparation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#DiffusionSeparation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Process to separate a gas mixture into its components by means of differences in the diffusion rates of the component molecules</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Dissolution -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Dissolution">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Combination"/>
        <rdfs:comment xml:lang="en">The mixing of a solid and a liquid with the formation of one new homogeneous phase (i.e. the solution).</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Distillation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Distillation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Distillation is the seperation of the constituents of liquid mixture via partial vaporization of the mixture and separate recovery of vapor and residue. (Perry)</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Dosing -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Dosing">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TransportCapability"/>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Drying -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Drying">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Drying is the process by which volatile materials are evaporated from a material to yield a solid product. (Perry)</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#ElectricalGasCleaning -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#ElectricalGasCleaning">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">The separation of dispersed particles from a gas phase by means of an electric field.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Electrodialysis -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Electrodialysis">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">A process which uses an electric field and two semipermeable membranes to separate dissociated species from undissociated species in a solvent. The electric field forces the dissociated ions through the membranes, while the nonpolar species remain in the solvent.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#ElectromagneticCrushing -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#ElectromagneticCrushing">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Fragmentation"/>
        <rdfs:comment xml:lang="en">The process of crushing solid material by applying electromagnetic fields that cause internal stresses.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Electrophoresis -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Electrophoresis">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Electrophoresis is the technique of separating electrically charged particles, particularly proteins, in a solution by passing an electric current through the solution. The rate of movement of the different components depends upon their charge, so that they gradually separate into bands. [neurolab.jsc.nasa.gov/glosseh.htm]</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Emulsification -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Emulsification">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Combination"/>
        <rdfs:comment xml:lang="en">The formation of a mixture of two liquids, such as oil and water, in which one of the liquids is in the form of fine droplets and is dispersed in the other.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#EnthalpyChange -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#EnthalpyChange">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TransformationCapability"/>
        <rdfs:comment xml:lang="en">EnthalpyChange in terms of unit operation considers a conversion of energy which often result in a change of the state of aggregation.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Extraction -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Extraction">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Extraction refers to both liquid-liquid extraction and leaching:
Liquid-liquid extraction is a process for separationg components in solution by their distribution between two immiscible liquid phases. Such a process can also be simply referred to as liquid extraction or solvent extraction; however, the latter term my be confusing because it also applies to the leaching of soluble substance from a solid. (Perry&apos;s) 
Leaching is the removal of a soluble fraction, in the form of a solution, from an insoluble, permeable solid phase with which it is associated. The separation usually involves selective dissolution. The soluble constituent may be solid or liquid (Perry&apos;s)</rdfs:comment>
        <rdfs:comment xml:lang="en">Might also be used for Solvent Extraction process.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Flashing -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Flashing">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Flashing utilized the state of phase equilibrium between a vapor and a liquid phase and the resulting material transport if a mixture with different fugacity is existent. In terms of unit operation this is usually accomplished in a vessel.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#FoamFormation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#FoamFormation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Combination"/>
        <rdfs:comment xml:lang="en">The combination of a liquid and a gas phase to a foam.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#FoamFractionation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#FoamFractionation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">A method of separation in which a component of the bulk liquid is preferentially adsorbed at the liquid/vapour (L/V) interface and is removed by foaming. (IUPAC Compendium of Chemical Terminology)</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Fragmentation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Fragmentation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TransformationCapability"/>
        <rdfs:comment xml:lang="en">Fragmentation in terms of unit operation is the breakup of solid material for further processing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Fusion -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Fusion">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Combination"/>
        <rdfs:comment xml:lang="en">The merging of two or more solid components by melting.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#GasCentrifugation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#GasCentrifugation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">The separation of a mixture of gases of different densities by centrifugal force.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#GasFiltration -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#GasFiltration">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Gas filtration is the separation of a gas-solids mixture by forcing the mixture through a porous barrier which retains most of the solid particulates contained in the mixture.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Grinding -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Grinding">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Fragmentation"/>
        <rdfs:comment xml:lang="en">The process of pulverizing solid material to fine-granular particles by applying mechanical forces</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#HeatingOfGases -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#HeatingOfGases">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TemperatureChangeOfGases"/>
        <rdfs:comment xml:lang="en">Increasing the temperature of a gas</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#HeatingOfLiquids -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#HeatingOfLiquids">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TemperatureChangeOfLiquids"/>
        <rdfs:comment xml:lang="en">Increasing the temperature of a liquid without changing ist state of aggregation</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#HeatingOfSolids -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#HeatingOfSolids">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#TemperatureChangeOfSolids"/>
        <rdfs:comment xml:lang="en">Increasing the temperature of a solid without changing ist state of aggregation.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#HoldingTemperature -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#HoldingTemperature">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#StorageCapability"/>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#Humidification -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#Humidification">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Combination"/>
        <rdfs:comment xml:lang="en">The mixing of a gas and the vapor of a liquid with the formation of a new homogeneous phase.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#HydrocyclonicSeparation -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#HydrocyclonicSeparation">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#Separation"/>
        <rdfs:comment xml:lang="en">Hydrocyclonic separation is a method of removing particles from a liquid stream by means of centrifugal forces.</rdfs:comment>
    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#ImpingementSeparation -->

