    </owl:Class>
    


    <!-- http://css.iat.rwth-aachen.de/OntoProCap#PhaseChangeOfGases -->

    <owl:Class rdf:about="http://css.iat.rwth-aachen.de/OntoProCap#PhaseChangeOfGases">
        <rdfs:subClassOf rdf:resource="http://css.iat.rwth-aachen.de/OntoProCap#PhaseChange"/>
    </owl:Class>
    


